diffs":[[0," PURPOSE"],[1," OF THIS SCRIPT"],[0,", YOU AR"]],"start1":1726,"start2":1726,"length1":16,"length2":31},{"diffs":[[0,"tation>\n"],[1,"::<recep>::GP Reception: no injuries, no allergies, no major medical problems, no current medications.\n"],[0,"::msg le"]],"start1":7443,"start2":7443,"length1":16,"length2":119},{"diffs":[[0,"ce mail\n"],[-1,"\n"],[0,";\n;\n;---"]],"start1":7649,"start2":7649,"length1":17,"length2":16},{"diffs":[[0,"cripts\n)"],[-1,""],[0,""],[1,"\n<<<<<<< HEAD\n=======\n    \n>>>>>>> 3b4b86c1b8116e1888c5656ba14581ec55a91142"],[0,"\n"]],"start1":15040,"start2":15040,"length1":9,"length2":84}]],"length":15124,"saved":false}
